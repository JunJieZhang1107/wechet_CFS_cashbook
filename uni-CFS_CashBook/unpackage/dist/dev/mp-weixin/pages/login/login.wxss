	text-align: center;
	font-weight: bold;
	margin-bottom: 30rpx;
}
.login-form{
	width: 80%;
	height: 600rpx;
	border: 1px solid;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	margin-left: 10%;
}
input{
	border: 1px solid;
	margin-top: 15rpx;
}
uni-forms-item{
	display: block;
	margin-top: 25rpx;
